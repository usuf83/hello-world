<?xml version="1.0" encoding="utf-8"?>
<Activity mc:Ignorable="sap" x:Class="RegisterUser" xmlns="http://schemas.microsoft.com/netfx/2009/xaml/activities"
 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
 xmlns:sap="http://schemas.microsoft.com/netfx/2009/xaml/activities/presentation"
 xmlns:ui="http://schemas.uipath.com/workflow/activities"
 xmlns:scg="clr-namespace:System.Collections.Generic;assembly=mscorlib"
 xmlns:sys="clr-namespace:System;assembly=mscorlib">
  <Sequence DisplayName="Register User">
    <ui:Click DisplayName="Click Register" Target.Selector="&lt;webctrl aaname='Register' tag='A' /&gt;" />
    <ui:TypeInto DisplayName="Type Username" Text="[email]" Target.Selector="&lt;webctrl id='email' tag='INPUT' /&gt;" />
    <ui:TypeInto DisplayName="Type Password" Text="[password]" Target.Selector="&lt;webctrl id='password' tag='INPUT' /&gt;" />
    <ui:Click DisplayName="Click Submit" Target.Selector="&lt;webctrl tag='BUTTON' /&gt;" />
    <ui:CheckAppState DisplayName="Check Registration Success" Target.Selector="&lt;webctrl tag='H1' innertext='Welcome' /&gt;">
      <ui:CheckAppState.Exists>
        <Assign DisplayName="Log Pass" To="[Result]" Value="Passed" />
      </ui:CheckAppState.Exists>
      <ui:CheckAppState.NotExists>
        <Assign DisplayName="Log Fail" To="[Result]" Value="Failed" />
      </ui:CheckAppState.NotExists>
    </ui:CheckAppState>
  </Sequence>
</Activity>
